package BVASM;

public class Xe {
	
	public String mauxe;
	public int tocdo;

	public Xe() {
		this.mauxe = "chua co mau";
		this.tocdo = 0;
	}
	
	public Xe(String mauxe, int tocdo) {
		this.mauxe = mauxe;
		this.tocdo = tocdo;
	}
	
	public void xuatThongTin() {
		System.out.printf("THONG TIN XE\n");
		System.out.printf("Mau xe: %s\n", this.mauxe);
		System.out.printf("Toc do: %d km/h\n", this.tocdo);
	}

}
